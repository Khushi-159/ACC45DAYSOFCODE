#include<iostream>
using namespace std;
int main()
{
    int year;
    cout<<"Enter the Year: ";
    cin>>year;
    if((yr%4==0) && (yr%100!=0))
        cout<<"It is a Leap Year";
    else if(yr%400==0)
        cout<<"It is a Leap Year";
    else
        cout<<"It is not a Leap Year";
