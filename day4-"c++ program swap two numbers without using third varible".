#include<iostream>
using namespace std;
int main()
{
int a,b;
cout<<"input first number:";
cin>>a;
cout<<"input second number:";
cin>>b;
cout<<"\n before swapping number"<<endl;
cout<<"a="<<a<<"b="<<b;
a=a+b;
b=a-b;
a=a-b;
cout<<"\n after swapping number:"<<endl;
cout<<"a="<<a<<"b="<<b;
return 0;
}
